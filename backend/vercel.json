{
  "version": 2,
  "builds": [
    {
      "src": "main_v3.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main_v3.py"
    }
  ],
  "env": {
    "BLOB_READ_WRITE_TOKEN": "@blob_read_write_token",
    "PRISMA_DATABASE_URL": "@prisma_database_url",
    "JWT_SECRET_KEY": "@jwt_secret_key",
    "API_DEBUG": "False"
  }
}
